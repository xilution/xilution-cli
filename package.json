{
  "name": "@xilution/xilution-cli",
  "version": "1.7.0",
  "description": "The Xilution command line interface. Provides command line abstractions for Xilution APIs.",
  "main": "./built/index.js",
  "bin": {
    "xln-cli": "./built/index.js"
  },
  "files": [
    "built"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xilution/xilution-cli"
  },
  "bugs": {
    "url": "https://github.com/xilution/xilution-cli/issues"
  },
  "homepage": "https://github.com/xilution/xilution-cli",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "release": "npm run build && semantic-release --ci false",
    "start": "ts-node ./src/index.ts",
    "test": "npm run test:unit",
    "test:unit": "jest --testPathPattern ./test/unit/",
    "verify": "npm run lint && npm run test"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "recursive-readdir": "^2.2.2",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/lodash": "^4.14.166",
    "@types/mime-types": "^2.1.0",
    "@types/recursive-readdir": "^2.2.0",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.12",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^5.20.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        }
      ],
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ]
      }
    },
    "generateNotes": {
      "preset": "angular",
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ]
      },
      "writerOpts": {
        "commitsSort": [
          "subject",
          "scope"
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Todd Brunia <tbrunia@gmail.com> (http://www.github.com/tbrunia)",
  "license": "MIT"
}
